<div class="client-list">
    <table>
        <thead>
            <tr>
                <th class="client-check"><input type="checkbox" /></th>
                <th class="client-info">Client Name</th>
                <th class="client-location">Location</th>
                <th class="client-phone">Phone</th>
                <th class="client-date">Date</th>
                <th class="client-status">Status</th>
                <th class="client-actions"></th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="client-check"><input type="checkbox" /></td>
                <td class="client-info">
                    <img class="client-image" src="~/images/Admin/Hans.svg" alt="Admin Photo." />
                    <div class="client-name">EPN Sverige</div>
                    <div class="client-email">info@domin.com</div>
                </td>
                <td class="client-location">Sweden</td>
                <td class="client-phone">+46 12 345 678 90</td>
                <td class="client-date">25-03-20</td>
                <td class="client-status">
                    <div class="badge success">active</div>
                </td>
                <td class="actions">
                    <i class="fa-solid fa-ellipsis" data-modal="true" data-target="#editClientModal"></i>
                </td>
            </tr>
        </tbody>
    </table>
</div>
